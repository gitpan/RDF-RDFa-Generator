  <Version rdf:about="http://purl.org/NET/cpan-uri/dist/RDF-RDFa-Generator/v_0-03">
    <dcs:changeset>
      <rdf:Description>
        <dcs:item>
          <dcs:Bugfix>
            <rdfs:label xml:lang="en">HTML::Pretty Fallback to sorting by object now works.</rdfs:label>
